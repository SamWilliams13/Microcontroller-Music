<Window x:Class="Microcontroller_Music.CreateTrackPopup"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Microcontroller_Music"
        mc:Ignorable="d"
        Title="CreateTrackPopup" Height="166.621" Width="369.459">
    <Grid Margin="10,10,2,2">
        <TextBox x:Name="TrackTitle" HorizontalAlignment="Left" Height="23" Margin="137,10,0,0" TextWrapping="Wrap" Text="Track " VerticalAlignment="Top" Width="202"/>
        <Label x:Name="label" Content="Track Title:" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label2" Content="Key:" HorizontalAlignment="Left" Margin="10,47,0,0" VerticalAlignment="Top"/>
        <ComboBox x:Name="KeyBox" HorizontalAlignment="Left" Margin="137,51,0,0" VerticalAlignment="Top" Width="202" SelectedIndex="0">
            <ComboBoxItem Content="Treble"/>
            <ComboBoxItem Content="Bass"/>
        </ComboBox>
        <Button x:Name="OKButton" Content="OK" HorizontalAlignment="Left" Margin="264,94,0,0" VerticalAlignment="Top" Width="75" Click="OKButton_Click"/>
    </Grid>
</Window>
